NAME = push_swap.a
CC = gcc
FLAGS = -Wall -Werror -Wextra
SRCS = 	includes/push_swap.h \
		functions/libft/ft_atoi.c \
		functions/libft/ft_strlen.c \
		functions/libft/ft_strcmp.c \
		functions/libft/ft_memset.c \
		functions/libft/ft_strjoin.c \
		functions/stack.c\
		functions/parsing.c \
		functions/get_operation.c\
		functions/operations/swap_stack.c\
		functions/operations/push_stack.c\
		functions/operations/rotate_stack.c\
		functions/operations/reverse_rotate_stack.c\
		functions/ps_functions/find_median.c\
		functions/ps_functions/sort_array.c\

$(NAME) : 	$(SRSC)
			@ $(CC) $(FLAGS) -c $(SRCS)
			@ ar rc $(NAME) *.o
			@ $(CC) $(FLAGS) checker.c $(NAME) -o checker -g
			@ $(CC) $(FLAGS) push_swap.c $(NAME) -o push_swap -g

all: 	$(NAME)

clean:
		@ rm -rf *.o

fclean: clean
		@ rm -rf $(NAME) checker includes/push_swap.h.gch

re: fclean $(NAME)


check:	re
	@ rm -rf *.o
	@ rm -rf $(NAME) includes/push_swap.h.gch
	./push_swap 3 1 4 2 5 8 6 7 | ./checker 3 1 4 2 5 8 6 7
# echo "sa\npb\npb\npb\nra\nrb\nrrr\nsa\npa\npa\npa" | ./checker 2 1 3 6 5 8


push:	re
	@ rm -rf *.o
	@ rm -rf $(NAME) includes/push_swap.h.gch
	./push_swap 408 191 176 189 71 283 403 221 123 497 115 104 36 32 343 184 118 107 351 366 292 297 116 235 202 128 277 229 149 396 78 148 198 224 435 486 179 215 83 426 498 8 347 3 496 244 101 252 241 79 378 67 26 74 90 260 269 14 431 478 477 135 223 493 272 412 310 282 273 286 356 494 457 119 483 386 391 52 17 171 96 336 181 15 398 100 10 495 326 411 390 288 300 168 16 137 167 42 470 363 178 267 409 57 309 121 442 11 314 39 194 151 381 344 246 88 129 134 258 454 271 180 61 73 330 109 94 280 448 126 81 487 311 105 72 174 237 113 362 199 6 56 165 157 291 153 360 193 231 341 301 399 117 209 474 424 102 21 155 146 175 439 365 296 89 103 232 53 405 86 464 46 488 216 150 110 359 350 447 364 480 142 354 440 418 40 31 327 387 324 306 158 220 63 68 346 261 294 302 385 62 500 70 430 458 4 445 136 332 230 377 164 93 259 434 98 463 352 443 370 353 499 333 163 368 425 132 389 268 465 196 444 432 99 328 37 247 401 312 64 195 274 345 7 141 298 173 460 400 388 95 337 34 473 329 201 120 481 27 251 406 355 446 222 228 335 342 256 108 41 112 145 170 87 451 476 85 44 138 2 461 490 492 225 287 213 255 203 491 249 392 383 162 69 437 206 374 423 160 233 166 453 208 130 227 207 278 452 358 313 469 321 43 482 262 410 257 466 266 240 124 65 77 204 323 197 154 210 348 38 24 371 303 205 214 147 159 429 111 242 393 234 414 188 318 182 427 319 33 307 28 373 25 293 217 339 143 5 489 19 186 218 45 238 250 55 156 462 60 468 152 80 416 455 50 263 131 144 279 239 91 122 419 295 331 315 479 372 281 127 47 192 402 58 84 18 92 76 275 29 12 172 384 322 219 417 226 308 450 212 438 320 421 456 75 375 48 140 376 125 161 340 243 289 299 66 236 475 395 133 397 20 394 284 407 415 357 187 276 35 349 382 54 467 472 379 334 248 13 459 183 265 114 449 97 82 441 369 254 367 404 422 471 413 1 177 139 185 433 338 200 59 270 22 290 253 304 316 245 30 190 23 305 325 428 317 9 264 169 436 211 485 380 420 106 361 484 285 51 49
